{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "npm install",
  "buildCommand": "npm run generate",
  "rewrites": [
    {
      "source": "/page1/",
      "destination": "/page2/index.html"
    },
    {
      "source": "/dynamic/(.*)",
      "destination": "/dynamic/[id]/index.html"
    }
  ]
}
